	<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Score</th>
      <th scope="col">Name</th>
      <th scope="col">Proof</th>
      <th scope="col">Comment</th>
    </tr>
  </thead>
  <tbody>
  	
   
   	<% items.forEach(function(item, index){ %>
     <tr>
   
      
      
   	  <td class = "scoreled"><%= item.Score %></td>
   	  <td class = "subrest"><%= item.Name %></td>
   	  <td class = "subrest"><a href= "<%= item.Proof %>" target="_blank"><%= item.Proof %></a></td>
   	  <td class = "subrest"><%= item.Comment %></td>
    </tr>
    <% }); %>
    
